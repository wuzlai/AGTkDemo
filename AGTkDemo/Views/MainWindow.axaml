<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:AGTkDemo.ViewModels;assembly=AGTkDemo"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AGTkDemo.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        xmlns:cefnet="clr-namespace:CefNet.Avalonia;assembly=CefNet.Avalonia"
        Title="AGTkDemo">

  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="*"></RowDefinition>
      <RowDefinition Height="100"></RowDefinition>
    </Grid.RowDefinitions>

    <cefnet:WebView  BorderBrush="Red" BorderThickness="1" x:Name="webView" Grid.Row="0"></cefnet:WebView>

    <Grid Grid.Row="1">
      <Button Grid.Column="0" HorizontalAlignment="Center" Width="120" Height="40" Content="Open File" Name="btnFileOpen"></Button>
    </Grid>


  </Grid>
</Window>
